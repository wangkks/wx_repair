.position{
    width: 100%;
    text-align: center;
}
.position image{
    width: 212rpx;
    height: 212rpx;
    margin: 100rpx auto;
}
.clockin{
    margin: 100rpx;
    width: 550rpx;
    height: 450rpx;
    border-left: 1rpx solid #8c9195;
    font-size: 30rpx;
    position: relative;
    color: #8c9195;
    padding-left: 30rpx;
    box-sizing: border-box;
}
.clockin .dian{
    position: absolute;
    left: -7rpx;
    width: 14rpx;
    height: 15rpx;
    border-radius: 50%;
    background-color: #8c9195;
}
.clockBg{
    height: 270rpx;
    position: relative;
}
.clockBg .bg{
    position: absolute;
    top: 40rpx;
    left: 200rpx;
    width: 152rpx;
    height: 152rpx;
}
.clockBg .time{
    position: absolute;
    top: 94rpx;
    left: 242rpx;
    color: #fff;
    font-size: 28rpx;
}
.clockBg .desc{
    position: absolute;
    left: 202rpx;
    top: 200rpx;
    font-size: 22rpx;
}
.clockBg text{
    border-radius: 24rpx;
    padding: 2rpx 8rpx;
    display: inline-block;
    margin-left: 10rpx;
}
.clockBg .success{
    border: 1rpx solid #007cbd;
    color: #007cbd;
    
}
.clockBg .catch{
    border: 1rpx solid #e5151f;
    color: #e5151f;
}
.detailInfo{
    display: flex;
    align-items: center;
    font-size: 26rpx;
    line-height: 45rpx;
}
.detailInfo image{
    width: 27rpx;
    height: 34rpx;
    margin-right: 10rpx;
}